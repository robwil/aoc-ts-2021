{
    "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-config-prettier"
    ],
    "rules": {
      "import-name": [false],
      "no-console": [false],
      "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
      "object-literal-sort-keys": false
    },
    "linterOptions": {
      "include": ["./**/*.ts"],
      "exclude": ["node_modules/**", "./**/*.js"]
    }
  }
  